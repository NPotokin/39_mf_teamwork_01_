{
  "name": "client",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist && tsc && vite build --config vite.config.ts",
    "build:ssr": "rimraf dist-ssr && tsc && vite build --config ssr.config.ts",
    "build:all": "yarn build:ssr && yarn build",
    "preview": "vite preview",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test:jest": "jest ./",
    "test:e2e:ci": "start-server-and-test 'yarn dev' http://localhost:3000 'cypress run --e2e'",
    "test:e2e:open": "start-server-and-test 'yarn dev' http://localhost:3000 'cypress open --e2e'"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.7",
    "@reduxjs/toolkit": "^2.2.6",
    "@types/node": "18.11.17",
    "antd": "^5.18.3",
    "axios": "^1.7.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.2",
    "eslint-config-prettier": "^8.5.0",
    "formik": "^2.4.6",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.2",
    "react-router": "^6.23.1",
    "react-router-dom": "^6.23.1",
    "sass": "^1.77.6",
    "serialize-javascript": "^6.0.2",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.8",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@vitejs/plugin-react": "^2.0.1",
    "classnames": "^2.5.1",
    "cypress": "^13.12.0",
    "cypress-vite": "^1.5.0",
    "eslint": "^8.23.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28",
    "jest-environment-jsdom": "^29.0.1",
    "lefthook": "^1.3.9",
    "prettier": "^2.7.1",
    "start-server-and-test": "^2.0.4",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.2",
    "vite": "^3.0.7"
  },
  "license": "MIT"
}
